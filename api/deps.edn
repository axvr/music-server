{:paths ["src" "resources"]
 :deps  {org.clojure/clojure     {:mvn/version "1.10.3"}
         ring/ring-core          {:mvn/version "1.9.3"}
         ring/ring-jetty-adapter {:mvn/version "1.9.3"}
         clout/clout             {:mvn/version "2.2.1"}
         caesium/caesium         {:mvn/version "0.14.0"}
