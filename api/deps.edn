         com.github.seancorfield/next.jdbc {:mvn/version "1.2.659"}
         com.github.seancorfield/honeysql  {:mvn/version "2.0.0-rc1"}}
 :aliases {:test {:extra-paths ["tests"]
                  :extra-deps {com.cognitect/test-runner
